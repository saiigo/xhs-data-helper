{
  "name": "xhs-helper",
  "version": "1.0.10",
  "description": "小红书数据助手 - 轻松保存你喜欢的小红书笔记",
  "main": "./out/main/main.js",
  "license": "CC-BY-NC-4.0",
  "author": {
    "name": "Your Name"
  },
  "scripts": {
    "dev": "cross-env ELECTRON_DISABLE_SANDBOX=1 electron-vite dev -w",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "start": "electron-vite preview",
    "postinstall": "electron-builder install-app-deps",
    "setup:python": "node scripts/download-python.js && node scripts/install-python-deps.js",
    "vite:build:app": "electron-vite build",
    "electron:build:win": "electron-builder --win --publish never",
    "electron:build:mac": "electron-builder --mac --publish never",
    "electron:build:linux": "electron-builder --linux --publish never",
    "electron:build:dir": "electron-builder --dir",
    "build:unpack": "run-s vite:build:app electron:build:dir",
    "build:win": "run-s setup:python vite:build:app electron:build:win",
    "build:mac": "run-s setup:python vite:build:app electron:build:mac",
    "build:linux": "run-s setup:python vite:build:app electron:build:linux",
    "release": "node scripts/release.js",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major",
    "release:dry": "node scripts/release.js --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/xhs-helper"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "electron",
      "electron-winstaller",
      "esbuild",
      "lzma-native"
    ]
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "better-sqlite3": "^12.4.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-updater": "^6.6.2",
    "lucide-react": "^0.541.0",
    "next-themes": "^0.4.6",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.7",
    "xlsx": "^0.18.5",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/rebuild": "^4.0.1",
    "@eslint/js": "^9.34.0",
    "@rushstack/eslint-patch": "^1.12.0",
    "@tailwindcss/vite": "^4.1.12",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.8",
    "@vitejs/plugin-react": "^5.0.1",
    "cross-env": "^10.0.0",
    "electron": "^37.3.1",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9",
    "electron-vite": "^4.0.0",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "framer-motion": "^12.23.12",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vite": "^7.1.3"
  },
  "packageManager": "pnpm@10.20.0+sha512.cf9998222162dd85864d0a8102e7892e7ba4ceadebbf5a31f9c2fce48dfce317a9c53b9f6464d1ef9042cba2e02ae02a9f7c143a2b438cd93c91840f0192b9dd"
}
